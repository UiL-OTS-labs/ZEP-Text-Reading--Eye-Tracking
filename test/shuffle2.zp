// shuffle2.zp
//
// Script to check the "random" module's item table shuffling function.
// This version is for a two-dimensional item table (used for between 
// subject's designs). For the one-dimensional item table see shuffle1.zp.

import std_defs;

import stimuli;
import random;


start()
{
    load_stimuli();

    int n;
    while (n < test_items.size)
    {
        shuffle_test_items(n);
        dump_test_items(n);
        n++;
    }

    terminate;
}
